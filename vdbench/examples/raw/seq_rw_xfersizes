sd=sd1,lun=/dev/rdsk/cxtxdxsx

wd=wd1,sd=sd*,seekpct=seq,rdpct=50

rd=rd1,wd=wd*,elapsed=120,interval=10,iorate=max,forxfersize=(4k-512k,d)
